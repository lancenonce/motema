{
"update_url": "http://www.amazon.es/",

  "name": "Motema Extension V1",
  "version": "1.0",
  "description": "Motema cobalt neutrality!",
  "permissions": [
    "storage",
    "alarms",
    "tabs","http://www.amazon.es/*","https://www.amazon.es/*","https://www.near.org/*"
  ],
  "content_scripts": [
    {
      "js": [
        "model/content_script.js"
      ],
      "matches": [
        "http://www.amazon.es/*","https://www.amazon.es/*","https://www.near.org/*"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "view/popup/popup.html",
    "default_icon": {
      "16": "images/motema_16.png",
      "32": "images/motema_32.png",
      "48": "images/motema_48.png",
      "128": "images/motema_128.png"
    }
  },
  "manifest_version": 2
}
